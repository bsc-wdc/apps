function READ_CL() {
  #
  PrintRPT("*  Processing Comman-Line Options...\n");
  #
  while ( ( getline < ARGV[1] ) > 0 ) {
	npar=split($0,parm,"::");
	for (r=1;r<=npar;r++) {
	  split(parm[r],elem,":=");
	  if (elem[1]~/COLOR$/) {
		ecolor=elem[2];
		if (!ChkColor(ecolor)) elem[2]=BG_COLOR;
		else if (tolower(ecolor)~/^fg(color)?$/) elem[2]=FG_COLOR;
	  } # if elem[1]
	  if (elem[2]!="!!!???") Var[elem[1]]=elem[2];
	} # for
  } # while
  ARGV[1]="";  
} # End of function READ_CL
