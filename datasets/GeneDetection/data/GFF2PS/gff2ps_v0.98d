  shwpst=TxtBool(OnOff(Default["show_positions"]) || OnOff(PLOT_FT[feature_,":","show_feature_positions"]) || OnOff(PLOT_FT[ng_,":","show_group_positions"]) || OnOff(PLOT_FT[sc,":","show_source_positions"]));
