#
# Timing gff2ps...
# TT_end=`date +%T`;
if [ $v34 -eq 1 ];
 then
perl -e '        $r = shift @ARGV; 
  $s = $r % 60;  $r = ($r - $s) / 60;
  $m = $r % 60;  $r = ($r - $m) / 60;
  $h = $r % 24;
  ($s,$m,$h) = (&fill_left($s,2,"0"),&fill_left($m,2,"0"),&fill_left($h,2,"0"));
print STDERR <<EOF;
**
********************************************
**  GFF2PS Execution Time:  $h:$m:$s
********************************************
EOF
sub fill_left { ($_[2] x ($_[1] - length($_[0]))).$_[0] }
' $SECONDS;
 fi;

