function Set_Line(nm,ext,    vn,cl,ln,fl,c,tmp) {
  if (ext=="gp_") {
	cl=PLOT_FT[nm,":","group_line_color"];
	vn=PLOT_FT[nm,":","group_line"];
    fl=gp[sq,sc,s,gp_cnt[nm],is_grouped];
	if ((vn in lnnm) && fl) ln=lnnm[vn];
