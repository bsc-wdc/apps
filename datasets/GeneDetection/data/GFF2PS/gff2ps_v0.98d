function ChkCharRG(chain) { # Avoiding problems with config file variable names for elements.
  if (chain=="") return ""; 
  else { # gsub(/[ \t]/,"",chain);
   gsub(/[\\]/,"\\\\",chain); gsub(/[\(]/,"\\(",chain); gsub(/[\)]/,"\\)",chain);
    gsub(/[\[]/,"\\[",chain); gsub(/[\]]/,"\\]",chain); gsub(/[\|]/,"\\|",chain);
    gsub(/[\{]/,"\\{",chain); gsub(/[\}]/,"\\}",chain); gsub(/[\/]/,"\\/",chain);
    gsub(/[\^]/,"\\^",chain); gsub(/[\$]/,"\\$",chain); gsub(/[\.]/,"\\.",chain);
    gsub(/[\?]/,"\\?",chain); gsub(/[\+]/,"\\+",chain); gsub(/[\*]/,"\\*",chain);
    return chain }
} # End of function ChkCharRG
