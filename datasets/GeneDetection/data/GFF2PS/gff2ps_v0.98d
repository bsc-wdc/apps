function Nuc_Scale(st,    ts) {
  ts=tolower(st);
  if (ts~/^m(ega)?(b(ases)?)?/) return 3;
  else if (ts~/^k(ilo)?(b(ases)?)?/) return 2;
  else return 1; # (ts~/^b(ases)?|default/);
}
