function chkun(st,    t,p) {
  t=tolower(st);
  if (gsub(/cm(s)?/,"",t)>0) { p=t" cm"; return p }
  else {
	if (gsub(/in(ch)?(s)?/,"",t)>0) { p=t" in"; return p }
	else {
	  if (gsub(/pt(s)?/,"",t)>0) return t;
	  else return "1 cm" }}
} # End of function chkun
