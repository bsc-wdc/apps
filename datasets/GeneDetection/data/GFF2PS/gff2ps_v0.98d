function calcpt(st,    t,p) { # return points
  t=tolower(st);
  if (gsub(/cm(s)?/,"",t)>0) { p=t*28.35; return p }
  else {
	if (gsub(/in(ch)?(s)?/,"",t)>0) { p=t*72; return p }
	else {
	  if (gsub(/pt(s)?/,"",t)>0) return t;
	  else return 0 }}
} # End of function calcpt
